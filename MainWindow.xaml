<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Safali"
        xmlns:Wpf="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf"
        x:Class="Safali.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="800" Icon="/icon.png">
    <Grid>
        <TextBox x:Name="address" Margin="175,6,155,0" TextWrapping="NoWrap" Text="TextBox" VerticalAlignment="Top" KeyDown="TextBox_KeyDown"/>
        <Button Content="↺" HorizontalAlignment="Right" Margin="0,5,100,0" VerticalAlignment="Top" Width="20" Click="Button_Click"/>
        <Button Content="+" HorizontalAlignment="Right" VerticalAlignment="Top" Height="20" Width="20" Margin="0,5,73,0" Click="Button_Click_1"/>
        <TabControl x:Name="tab" Margin="0,29,0,-2" SelectionChanged="tab_SelectionChanged">
            <TabItem Width="150" Header="000">
                <Wpf:WebView2 HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Source="https://04.si/" NavigationCompleted="WebView2_NavigationCompleted"/>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
